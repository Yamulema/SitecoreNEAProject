					<br>
					<b>Birthdate:</b>@Model.BirthDate
					<br>
					<b>City:</b>@Model.City
					<br>
					<b>Duplicate Email:</b>@Model.DuplicateEmail
					<br>
					<b>Email:</b>@Model.Email
					<br>
					<b>Email List:</b>@Model.EmailList
