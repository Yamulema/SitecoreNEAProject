<?xml version="1.0" encoding="utf-8" ?>
<controldata>
  <pages>
    <page name="TestSSOPage" urlpath="/UITest/TestSSO">
      <sections>
        <section name="Component">
          <controls>
            <control key="TestSSOPage_Component_Title" xpath="//h1[@class='title p-t-xs']"/>
            <control key="TestSSOPage_Component_Description" xpath="//h3[@class='subtitle text-light m-t-md m-b-md']"/>
            <!--control key="ProductTestPage_Component_Image" xpath="//img[@class='img-fit']"/-->
            <control key="TestSSOPage_Component_Share" xpath="//div[@class='attribution col-md-10 pull-right hidden-xs hidden-sm']"/>
          </controls>
        </section>
        <section name="CtaCold">
          <controls>
            <control key="TestSSOPage_CtaCold_SignIn" xpath="//a[@class='btn btn-blue btn-md m-r-sm']"/>
          </controls>
        </section>
        <section name="CtaWarm">
          <controls>
            <control key="TestSSOPage_CtaCold_PrimaryCta" xpath="//a[@id='product_cta_primary_action_button']"/>
            <control key="TestSSOPage_CtaCold_SecondaryCta" xpath="//a[@id='product_cta_secondary_action_button']"/>
          </controls>
        </section>
        <section name="CtaHot">
          <controls>
            <control key="TestSSOPage_CtaHot_PrimaryCta" xpath="//a[@id='product_cta_primary_action_button']"/>
          </controls>
        </section>
      </sections>
    </page>
  </pages>
</controldata>