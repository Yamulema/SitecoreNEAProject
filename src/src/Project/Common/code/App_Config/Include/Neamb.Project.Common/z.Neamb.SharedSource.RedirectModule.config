<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <!--  RedirectRootNode
            The module stores the rules as items in the Sitecore tree.  You can define a root node per domain.             
			  
			  <site domain="domain.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/SiteName"/>
			  
			  If there are no matches it uses the path provided in RedirectRootNode setting.
    -->
    <redirectMultiSiteSettings>
      <site domain="neamb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/NEAMB"/>
      <site domain="seiumb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/SEIUMB"/>
    </redirectMultiSiteSettings>
  </sitecore>
</configuration>
