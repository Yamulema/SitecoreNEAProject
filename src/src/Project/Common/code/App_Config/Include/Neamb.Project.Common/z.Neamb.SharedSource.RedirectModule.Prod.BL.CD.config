			<site domain="neamb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/NEAMB"/>
			<site domain="www.neamb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/NEAMB"/>
			<site domain="www9.neamb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/NEAMB"/>
			<site domain="seiumb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/SEIUMB"/>
			<site domain="www.seiumb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/SEIUMB"/>
			<site domain="www9.seiumb.com" redirectRootNode="/sitecore/system/Modules/Redirect Module/Redirects/SEIUMB"/>
		</redirectMultiSiteSettings>
	</sitecore>
</configuration>